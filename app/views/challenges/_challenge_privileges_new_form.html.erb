<!-- Form privilege start -->
<%= form_with(model: @privilege, local: true) do |f| %>
<!-- Form privilege end -->

  <!-- Label and input for challenge_id start -->
  <!-- Input for challenge_id start -->
  <%= f.hidden_field :challenge_id, value: @challenge.id %>
  <!-- Input for challenge_id end -->
  <!-- Label and input for challenge_id end -->

  <!-- Label and input for user_id start -->
  <!-- Input for "user_id" start -->
  <%= f.hidden_field :user_id, value: current_user.id %>
  <!-- Input for "user_id" end -->
  <!-- Label and input for user_id end -->

  <%= f.submit "Create privilege", class: "btn btn-block btn-outline-secondary" %>
<% end %>
